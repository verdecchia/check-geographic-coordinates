{
  "name": "check-geographic-coordinates",
  "version": "1.0.2",
  "description": "Simple utility (js and ts) to check if geographic coordinates (latitude, longitude) are valid.",
  "keywords": [
    "validate",
    "validator",
    "validation",
    "coordinates",
    "check",
    "geo",
    "geographic",
    "latitude",
    "longitude",
    "decimal degrees",
    "gps",
    "DD",
    "Geographic coordinate system",
    "es6",
    "javascript",
    "typescript",
    "js",
    "ts"
  ],
  "engines": {
    "node": ">=14"
  },
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "yarn del-cli dist/**/*",
    "mjs": "yarn tsc -m 'es2015' && mv dist/index.js dist/index.mjs && mv dist/index.d.ts dist/index.d.mts",
    "cjs": "yarn tsc",
    "build": "yarn clean && yarn mjs && yarn cjs",
    "test": "yarn build && yarn jest",
    "publish": "yarn build && yarn publish"
  },
  "repository": "https://github.com/verdecchia/check-geographic-coordinates.git",
  "author": "Marco Verdecchia <marco.verdecchia@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "del-cli": "^5.0.0",
    "jest": "^29.3.1",
    "typescript": "^4.9.4"
  }
}
